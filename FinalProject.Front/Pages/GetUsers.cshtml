@page
@model FinalProject.Front.Pages.GetUsersModel
@{
}
<table class="table table-striped">

	<tbody>
		
			@foreach (var user in Model.Users)
			{
			<tr>
				<th scope="row">Users</th>
				<td>@user.FirstName</td>
				<td>@user.LastName</td>
				<td>@user.Email</td>
				<td>@user.Address</td>
				<td>@user.Phone</td>
			</tr>
			}
			
	</tbody>
</table>